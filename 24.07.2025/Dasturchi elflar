n, m = map(int, input().split())
a = [list(map(int, input().split())) for _ in range(n)]

for i in range(n):
    row = []
    for j in range(m):
        base = a[i][j]
        if (i + j) % 2 == 0:
            if base % 2 == 1:
                base += 1
        else:
            if base % 2 == 0:
                base += 1
        row.append(str(base))
    print(" ".join(row))


